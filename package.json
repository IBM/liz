{
    "name": "liz",
    "version": "0.1.0",
    "private": true,
    "sideEffects": false,
    "author": "Daniel S. Haischt <daniel.haischt@de.ibm.com>",
    "repository": "git@github.ibm.com:linuxonz/liz.git",
    "bugs": "https://github.ibm.com/linuxonz/liz/issues",
    "homepage": "https://github.ibm.com/linuxonz/liz",
    "license": "MIT",
    "main": "index.js",
    "engines": {
        "node": "20.17.0",
        "npm": "10.8.2"
    },
    "scripts": {
        "commit": "git-cz",
        "dev": "vite",
        "prebuild": "npm run lint",
        "build": "vite build",
        "clean": "npx -y rimraf node_modules **/node_modules/ package-lock.json && npm i",
        "lint:eslint": "eslint --ext .js,.jsx --ignore-path .eslintignore --fix src",
        "lint:stylelint": "npx stylelint '**/*.scss'",
        "lint": "npm-run-all --parallel lint:eslint lint:stylelint",
        "test": "jest",
        "storybook:build": "build-storybook",
        "storybook": "start-storybook -p 6007",
        "preview": "vite preview",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d dist",
        "prepare": "husky install"
    },
    "dependencies": {
        "@carbon/colors": "11.24.0",
        "@carbon/ibm-products": "2.47.0",
        "@carbon/layout": "11.24.0",
        "@carbon/react": "1.64.1",
        "@carbon/styles": "1.63.1",
        "@carbon/themes": "11.38.0",
        "@carbon/type": "11.29.0",
        "@loadable/component": "5.16.4",
        "i18next": "23.14.0",
        "i18next-browser-languagedetector": "7.2.1",
        "i18next-http-backend": "2.6.0",
        "i18next-pseudo": "2.2.1",
        "is-ip": "5.0.1",
        "is-url-superb": "6.1.0",
        "is-valid-hostname": "1.0.2",
        "localstorage-slim": "2.7.1",
        "nanoid": "5.0.7",
        "react": "18.3.1",
        "react-copy-to-clipboard": "5.1.0",
        "react-dom": "18.3.1",
        "react-error-boundary": "4.0.13",
        "react-i18next": "14.1.3",
        "react-intersection-observer": "9.13.0",
        "react-router-dom": "6.26.1",
        "universal-user-agent": "7.0.2"
    },
    "devDependencies": {
        "@babel/cli": "7.24.8",
        "@babel/core": "^7.24.0",
        "@babel/eslint-parser": "7.25.1",
        "@babel/preset-env": "7.25.3",
        "@babel/preset-react": "7.24.7",
        "@commitlint/cli": "18.6.1",
        "@commitlint/config-conventional": "18.6.3",
        "@commitlint/cz-commitlint": "19.4.0",
        "@inrupt/jest-jsdom-polyfills": "3.2.2",
        "@storybook/addon-a11y": "7.6.20",
        "@storybook/addon-actions": "7.6.20",
        "@storybook/addon-docs": "7.6.20",
        "@storybook/addon-essentials": "7.6.20",
        "@storybook/addon-links": "7.6.20",
        "@storybook/builder-vite": "7.6.20",
        "@storybook/cli": "7.6.20",
        "@storybook/node-logger": "7.6.20",
        "@storybook/react": "7.6.20",
        "@testing-library/react": "14.3.1",
        "@types/jest": "29.5.12",
        "@types/react": "18.3.4",
        "@types/react-dom": "18.3.0",
        "@vitejs/plugin-react": "4.3.1",
        "babel-jest": "29.7.0",
        "babel-plugin-i18next-extract": "0.10.0",
        "babel-preset-vite": "1.1.3",
        "commitizen": "4.3.0",
        "enzyme": "3.11.0",
        "enzyme-adapter-react-16": "1.15.8",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-config-standard": "17.1.0",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-jest": "27.9.0",
        "eslint-plugin-n": "16.6.2",
        "eslint-plugin-promise": "6.6.0",
        "eslint-plugin-react": "7.35.0",
        "gh-pages": "6.1.1",
        "husky": "9.1.5",
        "i18next-hmr": "3.1.2",
        "identity-obj-proxy": "3.0.0",
        "inquirer": "^9.2.19",
        "jest": "29.7.0",
        "jest-environment-jsdom": "29.7.0",
        "lint-staged": "15.2.9",
        "npm-run-all2": "5.0.2",
        "prettier": "3.3.3",
        "react-test-renderer": "18.3.1",
        "sass": "1.77.8",
        "stylelint": "16.8.2",
        "stylelint-config-prettier-scss": "1.0.0",
        "stylelint-config-standard": "36.0.1",
        "stylelint-config-standard-scss": "13.1.0",
        "ts-jest": "29.2.4",
        "vite": "5.4.2",
        "vite-plugin-favicons-inject": "2.2.0"
    },
    "config": {
        "commitizen": {
            "path": "@commitlint/cz-commitlint"
        }
    },
    "lint-staged": {
        "**/*": [
            "prettier --write --ignore-unknown",
            "npm run lint:eslint",
            "npm run lint:stylelint"
        ]
    }
}
